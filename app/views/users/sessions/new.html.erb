<div>
  <a class="hiddenanchor" id="signin"></a>

  <div class="login_wrapper">
    <div class="animate form login_form">
      <section class="login_content">
        <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
          <h1>Login Usuário</h1>
          <div>
            <%= f.email_field :email, autofocus: true, class:"form-control", placeholder:"Email" %>
          </div>
          <div>
            <%= f.password_field :password, autocomplete: "current-password", class:"form-control", placeholder:"Senha" %>
          </div>
          <div>
            <%= f.submit "Entrar", class:"btn btn-default submit", style:"float: left; margin-left: 0px;" %>
            <%= link_to 'Esqueceu sua senha?', new_user_password_path, class:"reset_pass" %>
          </div>

          <div class="clearfix"></div>

          <div class="separator">
            <div>
              <h1><i class="fa fa-paw"></i> Gentelella Alela!</h1>
              <p>©2016 All Rights Reserved. Gentelella Alela! is a Bootstrap 3 template. Privacy and Terms</p>
            </div>
          </div>
        <% end %>
      </section>
    </div>
  </div>
</div>